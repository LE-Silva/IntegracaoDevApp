      <RecordSource>PARAMETERS
 [_numpedido] TEXT;

 SELECT
    PedidoDevApp.NumPedido as 'PedidoDevApp.NumPedido', 
    PedidoDevApp.CdCliente as 'PedidoDevApp.CdCliente',
    ClienteAtv2510.Nome as 'ClienteAtv2510.Nome',
    ClienteAtv2510.Cpf as 'ClienteAtv2510.Cpf',  
    PedidoDevApp.DtAbertura as 'PedidoDevApp.DtAbertura',
    PedidoDevApp.DtFechamento as 'PedidoDevApp.DtFechamento',
    CASE
        WHEN Status = 'F' THEN 'FECHADO'
        ELSE 'ABERTO'
    END AS Status,
    SUM(PedidoItemDevApp.Valor) as 'PedidoDevApp.Total'
 FROM PedidoDevApp
 JOIN ClienteAtv2510 ON ClienteAtv2510.CdCliente = PedidoDevApp.CdCliente
 JOIN PedidoItemDevApp ON PedidoDevApp.NumPedido = PedidoItemDevApp.NumPedido
 WHERE PedidoDevApp.NumPedido = [_numpedido]
 GROUP BY
    PedidoDevApp.NumPedido,
    PedidoDevApp.CdCliente,
    ClienteAtv2510.Nome,
    ClienteAtv2510.Cpf,  
    PedidoDevApp.DtAbertura,
